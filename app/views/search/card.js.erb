if ($('#ajax-search-card').size() === 0) {
  $('body').append('<div id="ajax-search-card"></div>');
} else {
  $('#ajax-search-card').html('');
}
$('#ajax-search-card').html('<%=j render partial: "ajax_container", collections: @data %>');

if ($('#ajax-search-card-aho').size() === 0) {
  $('body').append('<div id="ajax-search-card-aho"></div>');
} else {
  $('#ajax-search-card-aho').html('');
}
$('#ajax-search-card-aho').html('<%=j render partial: "ajax_container_failure", collections: @failure_data %>');
